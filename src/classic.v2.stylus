    bottom .3rem !important
  .el-menu
    padding-left 0
    padding-right 0
  // el-menu
  .el-menu
    radius(0)
    // margin .1rem 0
    background-color rgba(0,0,0,0)
    // color #9da4cb
    .el-submenu__title
      height .34rem
      line-height .34rem
      color #333
      background-color #e5e5e5
      &:hover
        background-color #e5e5e5
