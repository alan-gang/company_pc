      top .3rem
      bottom .3rem !important
    .dialog-page
      radius(0)
      top 0 !important
      shadow(none)



#app.classic.v2 .righter
  transform translateY(-50%)
  .ds-icon-classic
    display none
#app.classic.v2 .lefter
  top 1.05rem
  z-index 1900
  .el-menu
    padding-left .1rem
    padding-right .1rem
#app.classic.v2.game .lefter
