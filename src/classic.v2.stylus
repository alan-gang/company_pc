@import './var.stylus'
H = 1.04rem
// html
  // font-size 90px !important
#app.classic.v2 .new-home
  .dialog-container
    background-color #ededed
    position fixed
    top H
    left 0.5*H
    right 0.8*H
    bottom H !important
