/*
 *buttons class define
 */
.buttons
  .ds-button:first-child
    margin-right .1rem

.ds-button
  display inline-block
  padding 0 .22rem
  // height .3rem
  line-height .3rem
  text-align center
  color WHITE
  font-size .14rem
  border 0
  radius()
  box-shadow .02rem .02rem .02rem rgba(0, 0, 0, .2)
  font-shadow()
  user-select none
  cursor pointer
  box-sizing border-box
  vertical-align middle
  &.bold
    font-weight bold
  &.large
    padding 0 .3rem
  &.full
    padding-left 0
    padding-right 0
    width 100%
  &.small
    padding 0 .15rem
  &.x-small
    padding 0 .1rem
  &.xx-small
    line-height 1
    padding .035rem
  &._3rem
    width 3rem
  &.tall
    height .6rem
    line-height .6rem
    font-size .24rem
  &.oval
    border-top-left-radius 10% 50%
    border-top-right-radius 10% 50%
    border-bottom-left-radius 10% 50%
    border-bottom-right-radius 10% 50%
  
  &.primary
    background-color BLUE
    &:not(.disabled):hover
      background-color BLUE-HOVER
    &:not(.disabled):active
      background-color BLUE-ACTIVE 
  &.danger
    background-color DANGER
    &:not(.disabled):hover
      background-color DANGER-HOVER
    &:not(.disabled):active
      background-color DANGER-ACTIVE 
  &.positive
    bg-gradient(90deg, BLUE, #12cdbe) 
    &:not(.disabled):hover
      bg-gradient(90deg, #33b0ff, #24ddce) 
    &:not(.disabled):active
      background #0d8ddd
  &.high-positive
    bg-gradient(180deg, #ffbf00, #e98800) 
    &:not(.disabled):hover
      bg-gradient(180deg, #ffcb2e, #ff9500) 
    &:not(.disabled):active
      background #df960a
  
  &.gold
    color #911f1c
    text-shadow none
    background-color GOLD
    &:not(.disabled):hover
      background-color GOLD-HOVER
    &:not(.disabled):active
      background-color GOLD-ACTIVE 

  &.secondary
    color #606ba3
    text-shadow none
    bg-gradient(180deg, #f1f1f1, #e6e6e6)
    &:not(.disabled):hover
      bg-gradient(180deg, #fff, #f2f2f2)
    &:not(.disabled):active
      background #dedede
  
  &.cancel
    background-color #889095
    &:not(.disabled):hover
      background-color #98a0a5
    &:not(.disabled):active
      background-color #798287 
    

  &.outline
    font-size .12rem
    color #666
    background-color #fff
    border .01rem solid #ccc
    box-shadow .02rem .02rem .02rem rgba(0, 0, 0, .1)
    text-shadow none
    &:not(.disabled):hover
      border-color BLUE
    &:not(.disabled):active
      color #333
      background-color #f3f3f3
      border-color #aaa
      
  &.success
    background-color #65c014
    &:not(.disabled):hover
      background-color #6bcb16
    &:not(.disabled):active
      background-color #65c014
  
  &.text-button
    border 0
    background-color transparent
    box-shadow none
    text-shadow none
  &.light
    color LIGHT
    &:not(.disabled):hover
      color LIGHT-HOVER
  &.light.wg
    color #999
    &:not(.disabled):hover
      color #666
  &.blue
    color BLUE
    &:not(.disabled):hover
      color BLUE-HOVER
  &.green
    color GREEN
    &:not(.disabled):hover
      opacity .8    


  &.input
    padding 0 .04rem
    width .4rem
    text-align center
    pointer text
    .el-input__inner
    .el-input__inner:focus
    .el-input__inner:hover
      box-shadow none
      border none
      
    .el-input input.el-input__inner
    input
      box-shadow none
      border none
      width .3rem
      padding 0
      line-height .3rem
      height .3rem
      text-align center
    &:not(.disabled):active
      border none
      background-color #fff
      box-shadow 0 0 .05rem BLUE
      

  &.disabled
    cursor not-allowed
    background-image none
    box-shadow none
    text-shadow none
    &.primary
      color BLUE
      background-color transparent
      border .01rem solid BLUE
    &.secondary
      color LIGHT
      bg-gradient(180deg, #f1f1f1, #e6e6e6)

  &.isworking
    // font-size .18rem
    color GOLD !important
    background linear-gradient(135deg, #666, #666 10%, #333 10%, #333 20%, #666 20%, #666 30%, #333 30%, #333 40%, #666 40%, #666 50%, #333 50%, #333 60%, #666 60%, #666 70%, #333 70%, #333 80%, #666 80%, #666 90%, #333 90%, #333 100%)
    // background linear-gradient(135deg, BLUE, BLUE 10%, DANGER 10%, DANGER 20%, BLUE 20%, BLUE 30%, DANGER 30%, DANGER 40%, BLUE 40%, BLUE 50%, DANGER 50%, DANGER 60%, BLUE 60%, BLUE 70%, DANGER 70%, DANGER 80%, BLUE 80%, BLUE 90%, DANGER 90%, DANGER 100%)
    background-repeat repeat
    background-position 0 0
    background-size auto 100%
    animation animatedBackground 500s linear infinite

@keyframes animatedBackground {
  from {
    background-position 0 0
  }
  /*use negative width if you want it to flow right to left else and positive for left to right*/
  to {
    background-position 10000px 0
  }
}    

.ds-button-group
    // height .3rem
    line-height .28rem
    text-align left
    display inline-block
    vertical-align middle
    margin PWX
    radius()
    overflow hidden
    background-color #fff
    border .01rem solid #ccc
