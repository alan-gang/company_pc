.ds-checkbox
.el-checkbox__inner
  position relative
  top -.01rem
  display inline-block
  box-sizing border-box
  width .2rem
  height .2rem
  radius()
  shadow()
  margin-right .05rem
  vertical-align middle
  background-color WHITE
