@import './var.stylus'
@import './ie.stylus'


.el-tooltip__popper
  word-break break-all
  max-width 50%
/*
 *common css define
 */
a
  text-decoration none
  color #666
h1, h2, h3, h4, h5, p, dl, dd, dt
  margin 0
input
  outline none
pre
  white-space pre-wrap
  word-break break-all
.pl1
  padding-left .1rem
.pl2
  padding-left .2rem
.pl3
  padding-left .3rem
.pr1
  padding-right .1rem
.pr2
  padding-right .2rem
.pointer
  cursor pointer
::-webkit-input-placeholder
:-ms-placeholder
::-moz-placeholder
:-ms-input-placeholder
::-webkit-input-placeholder
    color #999 

.el-textarea.is-disabled .el-textarea__inner
  background-color #eee
  color #333
.modal .box-wrapper .box
  max-height 6rem
  overflow-y auto

.el-textarea.is-disabled .el-textarea__inner
  background-color #eee
  color #333
  
// el-table__empty-text
.el-table__empty-block
  position relative
  margin-top .2rem
  height 1.5rem
  background url($ASSETS/404/empty-table.png) center top no-repeat
  .el-table__empty-text
    vertical-align bottom
    transform: translate(-50%, 250%)
    
  
.el-table
  // &.bg-white
  .bg-white
    background-color #fff
    color #333
  .bg-light-blue
    background-color #f2faff
  .bg-light-danger
    background-color #fff2f3
    
  tr.bg-white
    td
      background-color #fff
  tr.border-top
    td
      border-top 2px solid #bbb
  .cell
    .ball
      display inline-block
      width .2rem
      height .2rem
      background-color BLUE
      color #fff
      line-height .2rem
      radius(50%)
      &.danger
        background-color DANGER

/*
 *user select
 */
&::selection
  // color WHITE
  // background-color BLUE
/*
 *scroll-bar
 */
*:not(:hover)
  &::scrollbar-thumb
    background-color transparent
    
::-webkit-scrollbar
  width .1rem
  height .1rem
  radius()
  background-color transparent
  
  &:hover
    background-color rgba(0,0,0,.2)

::-webkit-scrollbar-track::hover
  box-shadow inset 0 0 6px transparent
 
::-webkit-scrollbar-thumb
  radius()
  background-color rgba(0,0,0,.3)
  &:hover
    background-color rgba(0,0,0,.5)


// el-loading 
.el-loading-mask
  radius()
.dialog-page .el-loading-mask
  top TH
  border-top-left-radius 0
  border-top-right-radius 0
/*
 *el-breadcrumb
 */
 .el-breadcrumb__item__inner, .el-breadcrumb__item__inner a
   color BLUE

.el-breadcrumb__item__inner a:hover 
.el-breadcrumb__item__inner:hover
  color BLUE-HOVER

.el-breadcrumb__item:last-child .el-breadcrumb__item__inner, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner a, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner a:hover, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner:hover
  color #666
.el-breadcrumb__separator
  color #999
  
/*
 *el-message
 */
 .el-message
   position absolute


/*
 *rewrite el-dialog
 */
 DHH = .36rem
.el-dialog
  radius()
  background none
  overflow hidden
  .el-dialog__header
    padding 0 0 0 .2rem
    line-height DHH
    font-size .12rem
    background-color #d8d8d8
    // radius()
    // border-bottom-left-radius 0
    // border-bottom-right-radius 0
    .el-dialog__title
      font-size .12rem
      color #333
      font-weight bold
      // font-shadow()
    .el-dialog__headerbtn
      .el-icon
        text-align center
        width DHH
        height DHH
        line-height DHH
        color GREY
        &:last-child
          // border-top-left-radius 0
          // border-bottom-left-radius 0
          // border-bottom-right-radius 0
        &:hover
          background-color #f34
          color #fff
        &:active
          color #fff
          background-color #d40c1d
          
        // &:hover
        //   // color WHITE
        //   background #ccc
        //   // background-color DANGER-HOVER
        // &:active
        //   background #bbb
        //   // background-color DANGER-ACTIVE 
        
  .el-dialog__body
    padding 0

/*
 *rewrite el-popover
 */
.el-popover .popper__arrow
    display none
.el-popover
  min-width auto
  font-size .14rem
  padding PW .3rem
  background rgba(0, 0, 0, .6)
  radius()
  border 0
  &.false
    display none
  &.popover-blue
    padding 0
    min-width auto
    text-align center
    font-size .12rem
    color #b0cfe3
    background-color rgba(22, 113, 188, .95)
    shadow()
    font-shadow()
    
   
    
.el-icon-star-on
  &:before
    color GOLD

