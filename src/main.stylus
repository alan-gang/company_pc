        background-color BLUE
        color #fff
      
.ds-radio-label
  display inline-block
  padding 0 PW
  cursor pointer
  
  &:hover
    .ds-radio
      border-color rgba(255, 255, 255, .4)
      &.white
        border-color BLUE
 
  &.active
     .ds-radio
        border-color BLUE
        &:after
          width .07rem
          height .07rem 
          background-color BLUE
  &.disabled
    opacity .3

.forget-form .ds-radio-label
  color LIGHT
  &.active
    color WHITE
  
  
          
  
.ds-radio
  position relative
  top -.01rem
  display inline-block
  box-sizing border-box
  width .2rem
  height .2rem
  border .01rem solid rgba(255, 255, 255, .2)
  radius(50%)
  shadow()
  margin .05rem
  vertical-align middle
  &:hover
    border-color rgba(255, 255, 255, .4)
  &.active
    border-color BLUE
    &:after
      width .07rem
      height .07rem 
      background-color BLUE 
  &:after
    content ''
    position absolute
    left 50%
    top 50%
    width 100%
    height 100%
    radius(50%)
    transform translate(-50%, -50%)
    transition all .15s linear
  
  &.white
    border 1px solid #ccc
    background-color #fff
    &:hover
      border-color BLUE 
  &.disabled
    opacity .7
    
    
.ds-checkbox-label
.el-checkbox
  display inline-block
  margin-right .1rem
  color #666
  cursor pointer
  &:not(.disabled):hover
    .ds-checkbox
    .el-checkbox__inner
      border-color BLUE
      &.transparent
        border-color rgba(255, 255, 255, .4)
  &.active
    .ds-checkbox:after
        transform rotate(45deg) scaleY(1)
  
  &.disabled
    opacity .7

          
.el-checkbox__input.is-checked .el-checkbox__inner
  background-color #fff
  border-color #ccc
  
.ds-checkbox
.el-checkbox__inner
  position relative
  top -.01rem
  display inline-block
  box-sizing border-box
  width .2rem
  height .2rem
  radius()
  shadow()
  margin-right .05rem
  vertical-align middle
  background-color WHITE
  border .01rem solid #ccc
  &.transparent
    background-color transparent
    border .01rem solid rgba(255, 255, 255, .2)
  &:not(.disabled):hover
    border-color BLUE
  &.transparent:hover
    border-color rgba(255, 255, 255, .4)
  &.active
    border-color #ccc
    &:after
      transform rotate(45deg) scaleY(1)
      
  &.transparent.active
    border-color rgba(255, 255, 255, .4)
  
  
  

     

  &:after
    content ''
    position absolute
    width .05rem
    height .12rem
    box-sizing content-box
    border 3px solid BLUE
    border-bottom-right-radius .04rem
    border-left 0
    border-top 0
    left .05rem
    top .01rem
    transform rotate(45deg) scaleY(0)
    transition transform .15s cubic-bezier(.71,-.46,.88,.6) .05s
    transform-origin center
    
    
