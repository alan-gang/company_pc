/*
 *rewrite el-dialog
 */
 DHH = .36rem
.el-dialog
  radius()
  background none
  overflow hidden
  .el-dialog__header
    padding 0 0 0 .2rem
    line-height DHH
    font-size .12rem
    background-color #d8d8d8
    // radius()
    // border-bottom-left-radius 0
    // border-bottom-right-radius 0
