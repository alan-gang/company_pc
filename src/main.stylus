/*
 *rewrite el-table
 */
  table-bg(x = transparent)
    background-color x
    tr 
      background-color x
  table-header-bg(x = transparent)
    th 
      background-color x
      .cell
        background-color x
        
  table-header-color(x = #333)
    th
      .cell
        color x
  table-border(x = none)
    border x
    // .el-table--border td, .el-table--border th
    // .el-table--border th, .el-table__fixed-right-patch
    //   border x
      
    th
    th.is-leaf
    td
      border x
  
  has-border(x = 1px solid #d4d4d4)
    // border x
    .el-table__header td
      border-bottom x
      border-right 1px  solid #e6e6e6
    .el-table__header th
      border-bottom x
      
    .el-table__body td
      border-right 1px  solid #e6e6e6

    .el-table__header
      border-bottom 1px solid #bbb
      
      

    // .el-table__body th
    //   border-bottom 1px solid #666
    // .el-table__fixed-right-patch
    //   border-bottom x
      

  table-header-font-weight(x = normal)
    th 
      font-weight x 
       
  table-row-hover(x = #d8e6ef)
    .el-table--enable-row-hover .el-table__body tr:hover>td
      background-color x

    // tr.hover-row td
    //   background-color x
  
  table-row-selected(x = #f1e0e1)
    tr.selected-row td
      background-color x
  table-row-height(x = .2rem)
    td
      height auto
      .cell
        line-height x
   table-header-height(x = .2rem)
    th
      height auto
      .cell
        line-height x

  .el-table__fixed-header-wrapper thead div, .el-table__header-wrapper thead div
    color inherit
    background-color inherit
    
  .el-table::before
  .el-table::after
  .el-table__fixed::before
  .el-table__fixed::after
    display none
  .el-table--enable-row-hover .el-table__body tr:hover>td
    background-color #d8e6ef
  .el-table
    margin .05rem .02rem
    font-size .12rem
    color #666
    // 头部文字按钮
    th.actions .cell
      color BLUE 
      cursor pointer
    table-bg()
    table-header-bg()
    table-header-color()
    &.has-border
      has-border()
    &:not(.has-border)
      table-border()
    
        
    table-header-font-weight()
    table-row-selected()
    table-row-height()
    table-header-height()
    &.header-bold
      th
        .cell
          font-weight bold
    
    &.margin
      td
      height auto
        .cell
          margin .05rem 0
    
    &.nopadding
      .cell
      th > div
        padding 0.05rem .02rem
        line-height .2rem
  
  TABLEH = 6rem
  .el-table
    // width "calc(100% - %s)" % s
    .el-table__body-wrapper
      // padding-right .3rem
      // max-height TABLEH

  


  
