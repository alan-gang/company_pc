@import './var.stylus'
@import './ie.stylus'

/*
 *common css define
 */
a
  text-decoration none
  color #666
h1, h2, h3, h4, h5, p, dl, dd, dt
  margin 0
input
  outline none
.pl1
  padding-left .1rem
.pl2
  padding-left .2rem

.pr1
  padding-right .1rem
.pr2
  padding-right .2rem
.pointer
  cursor pointer
::-webkit-input-placeholder
:-ms-placeholder
::-moz-placeholder
:-ms-input-placeholder
::-webkit-input-placeholder
    color #999 


.el-textarea.is-disabled .el-textarea__inner
  background-color #eee
  color #333
  
// el-table__empty-text
.el-table__empty-block
  position relative
  margin-top .2rem
  height 1.5rem
  background url(./assets/404/empty-table.png) center top no-repeat
  .el-table__empty-text
    vertical-align bottom
    transform: translate(-50%, 250%)
    
  
.el-table
  // &.bg-white
  .bg-white
    background-color #fff
    color #333
  .bg-light-blue
    background-color #f2faff
  .bg-light-danger
    background-color #fff2f3
    
  tr.bg-white
    td
      background-color #fff
  tr.border-top
    td
      border-top 2px solid #bbb
  .cell
    .ball
      display inline-block
      width .24rem
      height .24rem
      background-color BLUE
      color #fff
      line-height .24rem
      radius(50%)
      &.danger
        background-color DANGER

/*
 *user select
 */
&::selection
  // color WHITE
  // background-color BLUE
/*
 *scroll-bar
 */
*:not(:hover)
  &::scrollbar-thumb
    background-color transparent
    
::-webkit-scrollbar
  width .1rem
  height .1rem
  radius()
  background-color transparent
  
  &:hover
    background-color rgba(0,0,0,.2)

::-webkit-scrollbar-track::hover
  box-shadow inset 0 0 6px transparent
 
::-webkit-scrollbar-thumb
  radius()
  background-color rgba(0,0,0,.3)
  &:hover
    background-color rgba(0,0,0,.5)


// el-loading 
.el-loading-mask
  radius()
.dialog-page .el-loading-mask
  top TH
  border-top-left-radius 0
  border-top-right-radius 0
/*
 *el-breadcrumb
 */
 .el-breadcrumb__item__inner, .el-breadcrumb__item__inner a
   color BLUE

.el-breadcrumb__item__inner a:hover 
.el-breadcrumb__item__inner:hover
  color BLUE-HOVER

.el-breadcrumb__item:last-child .el-breadcrumb__item__inner, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner a, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner a:hover, .el-breadcrumb__item:last-child .el-breadcrumb__item__inner:hover
  color #666
.el-breadcrumb__separator
