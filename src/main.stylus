      font-size .12rem
      color #333
      font-weight bold
      // font-shadow()
    .el-dialog__headerbtn
      .el-icon
        text-align center
        width DHH
        height DHH
        line-height DHH
        color GREY
        &:last-child
          // border-top-left-radius 0
          // border-bottom-left-radius 0
          // border-bottom-right-radius 0
        &:hover
          background-color #f34
          color #fff
        &:active
          color #fff
          background-color #d40c1d
          
        // &:hover
        //   // color WHITE
        //   background #ccc
        //   // background-color DANGER-HOVER
        // &:active
        //   background #bbb
        //   // background-color DANGER-ACTIVE 
        
  .el-dialog__body
    padding 0

/*
 *rewrite el-popover
 */
.el-popover .popper__arrow
    display none
.el-popover
  min-width auto
  font-size .14rem
  padding PW .3rem
  background rgba(0, 0, 0, .6)
  radius()
  border 0
  &.false
    display none
  &.popover-blue
    padding 0
    min-width auto
    text-align center
    font-size .12rem
    color #b0cfe3
    background-color rgba(22, 113, 188, .95)
    shadow()
    font-shadow()
    
   
    
.el-icon-star-on
