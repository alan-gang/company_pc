        display inline-block !important
        transform rotateZ(180deg)
        background #888
        color #333
        &:hover
          background #999
          
        &:first-child
          right -.2rem !important
        
      top .68rem
      background #313a64
      .el-dialog__wrapper
        background rgba(0,0,0,.5)
      &.game
        left 0
        transform none
        .el-menu-item-group__title
