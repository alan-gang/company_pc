        padding 0 0 PW PW
        height auto
      footer.hide-info + ul
      footer.shorter + ul
        top .45rem
      footer.hide-info.shorter + ul
        top .5rem
    
    .dialog-container
      &.has-lefter
        left 1.8rem
      left 0
      background #dedede
      top 1rem
      .dialog-page
        .followDetail-page-content
          bottom .6rem
          &:after
            display none
          
        .followDetail-page
          .mask
            background #666
          // position static
          .buttons
            bottom 0
            // position static !important
          
            
        GBG = #48525c
        BARGB = #343d46
        BTNBG = #5b646d
        // &.game-page 
        // &.game-page .game-content
