body.nds
  .popover-luckynumber
    display none
  .game-side-buttons
    display block !important    
  .new-game-info
    .game-luck-number-history
      position absolute
      top .5rem
      right 0
      padding 0
      width 2.8rem
      height .9rem
      overflow hidden
      &.expand
        height 5rem
        background #343d46 !important
        overflow auto
        
      .lucky-numbers
        padding 0 .05rem
        background none !important
        
      .NPER
        font-size .14rem
      
      .lucky-numbers
        
        & > .left
          min-width 2.6rem
          line-height .3rem
          height .3rem
        
        &.long
          & > .left
            min-width 2.6rem
            line-height .3rem
            height .6rem
          
        &.game-K3
          .dice
            transform perspective(100px) translateZ(-100px)
            &:nth-child(2)
            &:nth-child(3)
            &:nth-child(4)
              margin-left -.15rem
              
              // margin-left -.1rem
            
            // margin-top .10rem
          // .the-number 
          .side
            background #fff !important
            
        &:not(.game-K3)
          .the-number
          .number
            width: 0.20rem;
            height: 0.20rem;
            line-height: 0.20rem;
            font-size: .12rem
          .number
            color #ccc
            background: none !important
            border 1px solid #ccc
        
      
  .game-content > .lucky-numbers
    position absolute
    top .5rem
    left 3.4rem
    line-height .3rem
    z-index 1
    background none !important
    & > .left 
      width 3.5rem
      min-width 3rem
      & > span > span > div
        width 3.5rem
    .NPER
    .qi
    & > .right
    .timeout
      display none
    &.game-K3
      .number
        transform perspective(100px) translateZ(25px)
        margin-top .10rem
        &:not(:nth-child(3))
          margin-left .20rem
      // .the-number 
      .side
        background #fff !important
        
    &.long
      .left > span > span > div
        width 2.5rem
      .number
        margin-top .05rem
        background-color #ff7e00
        
      .the-number
        // margin-top .08rem
        
        background-color #ff7e00
        
    &:not(.long):not(.game-K3)
      .number
      .the-number
        background-color #ff7e00
        width: 0.60rem;
        height: 0.60rem;
        line-height: 0.60rem;
        font-size: .3rem
   
    // display none
  .game-info
    display none
    &.new-game-info
      display block
      
  #app .game-selection
    .circle
      // background rgba(255, 255, 255, .9)
      // color #666
      .dices.selected .dice .side
      &.default.selected
        background #ff7e00
  .all-rights
    text-shadow none
    color #efefef
          
  &:after
    content ''
    position absolute
    left 0
    top 0
    width 2rem
    height .6rem
